#. module: tailor_management
#: model:mail.template,body_html:tailor_management.email_template_ready
msgid ""
"<div style=\"font-family:Arial,sans-serif;\">\n"
"                    <p>Hello <t t-out=\"object.customer_id.name or 'Customer'\"/>,</p>\n"
"                    <p>Great news! Your order <b><t t-out=\"object.name\"/></b> is complete and ready for pickup.</p>\n"
"                    <p style=\"font-size:16px;\">Balance due: <b><t t-out=\"object.balance_due\"/> <t t-out=\"object.currency_id.symbol\"/></b></p>\n"
"                </div>\n"
"            "
msgstr ""
"<div style=\"font-family:Arial,sans-serif;\">\n"
"                    <p>مرحباً <t t-out=\"object.customer_id.name or 'عميلنا'\"/>،</p>\n"
"                    <p>أخبار رائعة! طلبك <b><t t-out=\"object.name\"/></b> مكتمل وجاهز للاستلام.</p>\n"
"                    <p style=\"font-size:16px;\">الرصيد المستحق: <b><t t-out=\"object.balance_due\"/> <t t-out=\"object.currency_id.symbol\"/></b></p>\n"
"                </div>\n"
"            "

#. module: tailor_management
#: model:mail.template,body_html:tailor_management.email_template_measurement
msgid ""
"<div style=\"font-family:Arial,sans-serif;\">\n"
"                    <p>Hello <t t-out=\"object.customer_id.name or 'Customer'\"/>,</p>\n"
"                    <p>It is time to schedule your measurements for order <b><t t-out=\"object.name\"/></b>.</p>\n"
"                    <p>Please contact us to confirm your availability.</p>\n"
"                </div>\n"
"            "
msgstr ""
"<div style=\"font-family:Arial,sans-serif;\">\n"
"                    <p>مرحباً <t t-out=\"object.customer_id.name or 'عميلنا'\"/>،</p>\n"
"                    <p>حان الوقت لجدولة موعد أخذ القياسات لطلبك <b><t t-out=\"object.name\"/></b>.</p>\n"
"                    <p>يرجى التواصل معنا لتأكيد موعد يناسبك.</p>\n"
"                </div>\n"
"            "

#. module: tailor_management
#: model:mail.template,body_html:tailor_management.email_template_cancelled
msgid ""
"<div style=\"font-family:Arial,sans-serif;\">\n"
"                    <p>Hello <t t-out=\"object.customer_id.name or 'Customer'\"/>,</p>\n"
"                    <p>This is to confirm that your order <b><t t-out=\"object.name\"/></b> has been cancelled.</p>\n"
"                </div>\n"
"            "
